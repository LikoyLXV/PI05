# Узел списка
class Node:
    def __init__(self, data):
        self.data = data
        self.next = None

# Односвязный список
class LinkedList:
    def __init__(self):
        self.head = None

    def push_front(self, val):
        new = Node(val)
        new.next = self.head
        self.head = new

    def push_back(self, val):
        new = Node(val)
        if not self.head:
            self.head = new
            return
        cur = self.head
        while cur.next:
            cur = cur.next
        cur.next = new

    def pop_front(self):
        if self.head:
            self.head = self.head.next

    def print(self):
        cur = self.head
        while cur:
            print(cur.data, end=" ")
            cur = cur.next
        print()

# Тест
lst = LinkedList()
lst.push_back(1)     # [1]
lst.push_front(2)    # [2, 1]
lst.push_back(3)     # [2, 1, 3]
lst.print()          # 2 1 3
lst.pop_front()      # [1, 3]
lst.print()          # 1 3
